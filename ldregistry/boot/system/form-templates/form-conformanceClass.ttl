@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix version:  <http://purl.org/linked-data/version#> .
@prefix ldp:  <http://www.w3.org/ns/ldp#> .
@prefix reg:  <http://purl.org/linked-data/registry#> .
@prefix ui:   <http://purl.org/linked-data/registry-ui#> .
@prefix spec:		<http://www.opengis.net/ont/modspec#> .

<conformance-class> a ui:FormTemplate;
    rdfs:label "ConformanceClass"@en;
    ui:tooltip "Define a single spec:ConformanceClass"@en;
    ui:prototype [a spec:ConformanceClass];
    ui:formFields (
      [a ui:FormField; rdfs:label "id"; skos:notation "id";
            ui:tooltip "ID ('notation') for Conformance Class (sub-register) in context of this register"@en;
            ui:property reg:notation;
            ui:required true ;
            ui:fieldType ui:labelField]
      [a ui:FormField; rdfs:label "label"; skos:notation "label";
            ui:tooltip "Label for the Conformance Class"@en;
            ui:property rdfs:label;
            ui:required true ;
            ui:fieldType ui:labelField]
      [a ui:FormField; rdfs:label "requirements"; skos:notation "requirements";
            ui:tooltip "Requirements Class for which this Conformance Class provides tests"@en;
            ui:property spec:requirementsTested;
            ui:required true ;
            ui:fieldType ui:anyURIField]
      [a ui:FormField; rdfs:label "dependency"; skos:notation "dependency";
            ui:tooltip "Dependencies for this Conformance Class - i.e. previously defined conformance classes or other resources on which this class depends"@en;
            ui:property spec:dependency;
            ui:fieldType ui:anyURIField]
      [a ui:FormField; rdfs:label "test"; skos:notation "test";
            ui:tooltip "Test in this Conformance Class (if already defined)"@en;
            ui:property rdfs:member;
            ui:fieldType ui:anyURIField]
    );
    .